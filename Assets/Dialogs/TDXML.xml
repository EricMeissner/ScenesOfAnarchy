<root>
  <DIALOG class="VDialog" name="MainMenu" fullscreen="FALSE" backgroundcolor="51,51,51,200" size="200,100" startPosition="ScreenCenter">

	<!-- NOTES:
	
	Position for coordinates  = "X,Y"
	- (0,0) Left top of the Parent Box
	- (X+,Y) --move to the right
	- (X-,Y) --move to the left
	- (X,Y+) --move up
	- (X,Y-) --move down
	
	-Keep Everything aligned on the X-coordinate
	
	
	-->
		
	<!-- Project Name -->


	<!--Tower of Doom Scene Button -->
	
	<control class="VPushButton" ID="ADD" pos="0,50" size="100,50" dialogResult="ADD" enabled="true" tooltiptext="Add Objects" taborder="6">
      <text text="ADD SHIT" font="MenuFont" color="180,180,180,255" valign="CENTER" halign="LEFT">
        <statemodifier state="mouseOver" color="230,230,230,255"/>
        <statemodifier state="selected"  color="255,255,255,255"/>
        <statemodifier state="disabled"  color="120,120,120,255"/>
      </text>
    </control>
	
	
  </DIALOG>
</root>
